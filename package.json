{
    "name": "@loopkit/javascript",
    "version": "1.0.0",
    "description": "LoopKit JavaScript SDK for tracking events and user analytics",
    "main": "dist/loopkit.js",
    "module": "dist/loopkit.esm.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "rollup -c",
        "build:watch": "rollup -c -w",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint src/**/*.js",
        "format": "prettier --write src/**/*.js",
        "prepare": "npm run build"
    },
    "keywords": [
        "analytics",
        "tracking",
        "events",
        "loopkit",
        "javascript",
        "sdk"
    ],
    "author": "LoopKit",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/preset-env": "^7.23.0",
        "@rollup/plugin-babel": "^6.0.0",
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^15.2.0",
        "@rollup/plugin-terser": "^0.4.0",
        "eslint": "^8.50.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "prettier": "^3.0.0",
        "rollup": "^4.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/loopkit/javascript-sdk.git"
    },
    "bugs": {
        "url": "https://github.com/loopkit/javascript-sdk/issues"
    },
    "homepage": "https://github.com/loopkit/javascript-sdk#readme"
}